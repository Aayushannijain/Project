<?xml version="1.0" encoding="utf-8"?>
<configuration>

 
	<configSections>
		<!-- Enterprise Library Data Block Section -->
		<section name="system.data"
                 type="System.Data.Common.DbProviderFactoriesConfigurationHandler, 
                   Microsoft.Practices.EnterpriseLibrary.Data, 
                   Version=6.0.0.0, 
                   Culture=neutral, 
                   PublicKeyToken=null" />
	</configSections>



	<!-- Connection string for MySQL -->
	<connectionStrings>
		<add name="Local"
            connectionString="server=192.168.50.53;database=ayush;uid=dev;pwd=DevDb@321;"
             providerName="MySql.Data.MySqlClient" />
	</connectionStrings>

	<system.data>
		<DbProviderFactories>
			<remove invariant="MySql.Data.MySqlClient" />
			<add name="MySQL Data Provider"
                 invariant="MySql.Data.MySqlClient"
                 description=".NET Framework Data Provider for MySQL"
                 type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=8.0.23" />
		</DbProviderFactories>
	</system.data>

</configuration>